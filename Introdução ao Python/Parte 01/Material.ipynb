          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Condicional NOT: (not c)\n",
            "True\n",
            "Condicional OR: (a or b)\n",
            "True\n",
            "Condicional AND: (a and b)\n",
            "False\n"
          ]
        }
